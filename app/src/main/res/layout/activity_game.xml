<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".GameActivity" android:layout_gravity="center"
android:orientation="vertical">


    <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/levelbuton" android:id="@+id/imageView5"
                android:layout_gravity="center|top"/>
        <TextView

                android:text="Level 1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView6" android:textSize="40sp"
                android:layout_gravity="center|top"/>
    </FrameLayout>
    <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:gravity="center" android:id="@+id/layoutTable">


    </TableLayout>
</LinearLayout>